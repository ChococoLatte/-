<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" id="viewport" content="width=device-width, height=device-height, initial-scale=1" >
  <title>구글맵</title>
  <style>
    * {
      margin: 0;
      padding: 0;
    }
    html,body {
      width: 100%;
      height: 100%;
    }
    #map{
      width: 100%;
      height: 95%;
    }                                                                                       
  </style>
                                                                         
  <script src="https://maps.googleapis.com/maps/api/js?key="></script>
                                                        
  <script>
     function onLoad() {
    initMap();
}
	 
var map;
var centerLocation = {lat: <%= paramLatitude %>, lng: <%= paramLongitude %>};

var eateryLocation = {lat: <%=result.geometry.coordinates[1] %>,lng: <%=result.geometry.coordinates[0] %>};
      
function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
        center: centerLocation,
        zoom: 15
    });
    
var myMarker = new google.maps.Marker({
                      position:centerLocation,
                      icon:'/public/myposition.png',
                      animation:google.maps.Animation.BOUNCE
        });
        myMarker.setMap(map);
 
   
var eateryMarker= new google.maps.Marker({
    position:eateryLocation,
    icon:'/public/restaurant.png'
});
    
eateryMarker.setMap(map);
   
}

 </script>
                                                    
</head>
 <body onload = "onLoad()">
    <p>음식점 조회</p>
    <div id = "map"></div>
</body>
</html>